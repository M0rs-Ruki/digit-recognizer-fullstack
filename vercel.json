{
    "version": 2,
    "builds": [
      {
        "src": "Backend/app.js",
        "use": "@vercel/node"
      },
      {
        "src": "ai/app.py",
        "use": "@vercel/python"
      },
      {
        "src": "Frontend/package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "Frontend/dist"
        }
      }
    ],
    "routes": [
      {
        "src": "/api/test-python",
        "dest": "Backend/app.js"
      },
      {
        "src": "/api/predict",
        "dest": "Backend/app.js"
      },
      {
        "src": "/api/ai/.*",
        "dest": "ai/app.py"
      },
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }