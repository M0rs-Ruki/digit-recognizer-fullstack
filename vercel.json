{
    "version": 2,
    "builds": [
        {
            "src": "Backend/app.js",
            "use": "@vercel/node"
        },
        {
            "src": "ai/app.py",
            "use": "@vercel/python"
        },
        {
            "src": "Frontend/package.json",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "dist"
            }
        }
    ],
    "routes": [
        {
            "src": "/api/predict",
            "dest": "Backend/app.js"
        },
        {
            "src": "/api",
            "dest": "Backend/app.js"
        },
        {
            "src": "/api/ai/(.*)",
            "dest": "ai/app.py"
        },
        {
            "src": "/assets/(.*)",
            "headers": {
                "cache-control": "s-maxage=31536000, immutable"
            },
            "dest": "/assets/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ]
}